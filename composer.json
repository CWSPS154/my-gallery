{
    "name": "cwsps154/my-gallery",
    "description": "Filament package for gallery",
    "keywords": [
        "cwsps154",
        "laravel",
        "filament",
        "filament-packages",
        "filament-plugins",
        "filament-gallery"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CWSPS154\\MyGallery\\": "src/",
            "CWSPS154\\MyGallery\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "CWSPS154",
            "email": "codewithsps154@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "joshembling/image-optimizer": "^1.4",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CWSPS154\\MyGallery\\MyGalleryServiceProvider"
            ]
        }
    }
}
